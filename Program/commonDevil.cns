; Common state file for M.U.G.E.N beta 2002; System variables usage
; ----------------------
; Var name    Type   Purpose
; --------    ----   -------
; sysvar(0)   Temp   Set by state 5081 (HITL_SLIDE) to 1 to prevent
;                    showing ground hit frame in state 5110 (HIT_LIEDOWN).
;                    Used by guarding states.
; sysvar(1)   Temp   Used by state 40 (Jump Start) amd 45 (Air Jump Start)
;                    to keep track of which way to jump.
;                    Used by state 5100 and 5110 to remember downward
;                    velocity of player just before hitting the ground.
;                    Used in state 5080 for storing the anim # to display.

; The following states have special handling within MUGEN, and should be
; overridden with caution:
; Jump Start (40): airjumping bookkeeping at time = 0
; All guard states (120-155): guard handling throughout state
; Hit_trip, shaking (5070): defence increases at time = 0
; Hit_bounce (5100): defence increases at time = 0, player becomes
;                    invincible if within 10 ticks of getting up
; Hit_liedown (5110): player changes to get-up state (5120) when ready
; Hit_getup (5120): player's defence is restored to normal at time = 0
; Hitfall_recover (5200): player's defence is restored at time = 0
; Hitfall_airrecover (5210): player's defence is restored at time = 0

;---------------------------------------------------------------------------
; Stand
[Statedef 0]
type = U
movetype = U
physics = U
hitdefpersist = 1
movehitpersist = 1
hitcountpersist = (name != "Finaltly_Death_Claw")
sprpriority = -1*!ishelper

[State 0]
type = ChangeState
triggerall =(!ishelper&&name!= "Finaltly_Death_Claw" || ishelper&&root,name!= "Finaltly_Death_Claw")&&!SelfAnimExist(-123123123)
trigger1 = prevstateno != [1000043,1000046]
trigger1 = prevstateno != [666666000,666666200]
trigger2 = time || ctrl
value = ifelse(ishelper,666666000,1000043)
persistent = 256
ignorehitpause = 1

[state 0]
type = ctrlset
trigger1 = name != "Finaltly_Death_Claw"
value = 1
ignorehitpause = 1

[State ]
type = VarSet
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = stateno!= sysfvar(1)
sysvar(3) = -1
ignorehitpause = 1

[State ]
type = VarSet
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = sysfvar(1)!=stateno
sysfvar(1) = stateno
ignorehitpause = 1

[State ]
type = VarSet
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = sysfvar(1)=stateno
sysvar(3) = sysvar(3)+1
ignorehitpause = 1

[State 0]
type = statetypeset
trigger1 = name = "Finaltly_Death_Claw"
statetype = S
movetype = I
physics = S
ignorehitpause = 1

[State 0]
type = statetypeset
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = sysvar(4) = 2
trigger1 = roundstate > 1
trigger1 = alive = 0
trigger1 =!hitpausetime
movetype = A
ignorehitpause = 1

[State ]
type = MoveHitReset
trigger1 = name = "Finaltly_Death_Claw"
ignorehitpause = 1

[State 0, 1]
type = ChangeAnim
triggerall = name = "Finaltly_Death_Claw"&&(sysvar(4) = [0,1])
trigger1 = (Anim != [0,1]) && Anim != 5
trigger2 = Anim = 5 &&!AnimTime ;Turn anim over
value = 0
ignorehitpause = 1

[State 0, 1]
type = ChangeAnim
trigger1 = name = "Finaltly_Death_Claw"&&sysvar(4) = 2
;trigger1 = (Anim != [0,1]) && Anim != 5
;trigger2 = Anim = 5 &&!AnimTime ;Turn anim over
value = 1
elem = (sysvar(3)%198)/6
ignorehitpause = 1

[State 0, 1]
type = ChangeAnim
triggerall = name = "Finaltly_Death_Claw"&&(sysvar(4)!= [0,2])
trigger1 = Anim != 2 && Anim != 5
trigger2 = Anim = 5 &&!AnimTime ;Turn anim over
value = 2
ignorehitpause = 1

[state ,凍結獲取]
type = hitdef
triggerall = name = "Finaltly_Death_Claw"
trigger1 = sysvar(4) = 2
trigger1 = roundstate > 1
trigger1 = alive = 0
trigger1 =!hitpausetime
attr = ,HP
pausetime = 10,0
numhits = 0
sparkno = -1
hitsound = -1
affectteam = F
ignorehitpause = 1

[state ]
type = ctrlset
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = sysvar(4) = 2
value = (fvar(1)>200)*(roundstate = 2)
ignorehitpause = 1

[state ]
type = ctrlset
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = sysvar(4) = 3
value = 1
ignorehitpause = 1


[State ]
type = MoveHitReset
trigger1 = name = "Finaltly_Death_Claw"
ignorehitpause = 1

[State PosSet]
type = PosSet
trigger1 = name = "Finaltly_Death_Claw"
y = 0
ignorehitpause = 1

[State 0, AfterImage]
type = null;AfterImage
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = animelemtime(1) = 0
time = -1
length = 20
PalBright   = 256,0,0
PalContrast = 256,0,0
PalAdd      = 256,0,0
Trans = Add
TimeGap  = 1
framegap = 1
ignorehitpause = 1

[State 0, 2]
type = VelSet
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = animelemtime(1) = 0
y = 0
ignorehitpause = 1

[State 0, 3] ;Stop moving if low velocity or 4 ticks pass
type = VelSet
triggerall = name = "Finaltly_Death_Claw"
trigger1 = abs(vel x) < 2
trigger2 = animelemtime(1) = 4
x = 0
ignorehitpause = 1

[State 0, 5] ;Are you dead?
type = ChangeState
trigger1 = name = "Finaltly_Death_Claw"
trigger1 = sysvar(4) = [0,1]
trigger1 = !alive
value = 5050
ignorehitpause = 1

[State ]
type = ChangeState
triggerall = name = "Finaltly_Death_Claw"
trigger1 = sysvar(4)!= 0
trigger1 = roundstate > 2
trigger2 = sysvar(4)!= [0,2]
value = 120
ignorehitpause = 1

