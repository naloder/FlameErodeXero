type = hitdef	;该State的类型。类型：攻击
trigger1 = name = "Finaltly_Death_Claw"
;trigger1 = movehit = 0	;触发条件，还可增加trigger2，trigger3等
attr = S,NA	;自己的攻击定义。x为S-站立,C-下蹲或A-空中。xx的第一个x为N-普通,S特殊或H-无敌，第二个为A-直接攻击,P-发射道具或T-投技。
hitflag = MAFDP	;决定对手在哪些状态下可被命中。可选：H-上段,L-下段,A-空中,M-中段(M=HL),F-在目标被打到空中时可追打,D-在目标被打倒后可追打。
guardflag = MA	;决定对手怎么才能防御此攻击。H-上段,L下段,A-空中或M-中段（M=HL）。
affectteam = E	;决定是否击中友军（滑稽），B-全部,E-敌方,F-我方。默认E。
animtype = diagup	;对手受击后的状态。light为轻,medium为中,hard为重,back是击倒,up是垂直飞起,diagup是斜后飞起。默认light。
air.animtype = medium	;同上，只是在对手跳在空中时起作用。
fall.animtype = medium	;同上，只是在对手被打往后飞时起作用。
priority = 1,hit	;攻击的优先度与同优先级互杀的反应。优先度为1-7，默认为4，而反应有hit,miss,dodge。hit对hit互打,对miss被打,其余都不被打。
damage = ceil(((enemy,const(data.defence)*0.01)*enemy,lifemax*0.050)),ceil(((enemy,const(data.defence)*0.01)*enemy,lifemax*0.010))	;对手不防御会受到xx伤害,对手防御住会受到多少伤害。
pausetime = 0,6	;第一个xx为自己人物打中后的停顿时间，第二个xx为对手被打中后的振动时间。
guard.pausetime = 0,11	;同上，只是在对手防御时起作用。
sparkno = -1	;火花的anim号，想要指定某个号就加上S。例：Sxxx。默认为0。
guard.sparkno = -1	;同上，只是被防御时出现的火花号。
sparkxy = 0,0	;火花的出现位置（x,y）。x负数深入对手,正数远离对手。y负数在上,正数在下。默认为0,0。
hitsound = S16,3	;打击音效（组，索引）。要播放自己的音效在组前加s。默认为0,0。
guardsound = S200,2	;同上，只是攻击被防御的音效。
ground.type = low	;对手站地上时攻击类型。high为打头,low为打腹部,trip为扫腿,none无影响。如果animtype是back，high和low不起作用。默认为high。
air.type = low	;同上，不过是对手在空中时起作用。省略为ground.type。
ground.slidetime = 20	;对手向后滑的时间，单位为ticks。在地面上生效。默认为0。
guard.slidetime = 4	;同上，不过在被防御时起效。默认为ground.slidetime。
ground.hittime = 25	;对手处在被击中状态的时间,单位为ticks。在地面上生效,默认为0。
guard.hittime = 4	;同上，不过在被防御时起效。默认为ground.hittime。
air.hittime = 20		;同上，不过在对手被击飞时起效。fall为1时无效，默认为20.
yaccel = 1	;给对手的竖直方向的加速度，默认为0。
ground.velocity = -20,-20	;对手在地面被击中时给对手的初速度。默认为0,0。
guard.velocity = 4	;同上，不过在被防御时起效。
air.velocity = -1,-5	;同上，不过在对手在空中时起效。
down.velocity = -4,-10	;同上，不过在对手倒地时起效。默认同上。
down.hittime = 10	;对手倒地被打滑动时间。上面的y非0则省略此参数。
airguard.velocity = 10,3	;同上上，不过在对手空防时起效。
ground.cornerpush.veloff = 4	;对手在地图边缘被击中自己后滑速度。省略则在attr的x为A时，默认为0。否则为guard.velocity的1.3倍。
air.cornerpush.veloff = 2	;同上，不过在对手在空中时起效。默认同上。
down.cornerpush.veloff = 1	;同上，不过在对手下蹲时起效。
guard.cornerpush.veloff = 6	;同上，不过在对手防御住攻击时起效。
airguard.cornerpush.veloff = 1	;同上，不过在对手空防时起效。
p1sprpriority = 1	;人物贴图图层。越大越前。
p2sprpriority = 0	;目标贴图图层。同上。
p1facing = 0	;设为-1则在击中对手时转身。
fall = 0	;设为1则令对手被击中后进入fall（坠落）状态。
air.fall = 1	;同上，默认同上。
fall.recover = 0	;设为0则让对方在fall时无法恢复。默认为1。
fall.damage = 0	;对手落地时受伤。
kill = 1	;设为0则即使对方血再少也不会被KO。默认为1。
guard.kill = 1	;同上，只是在防御时起效。
